{
    "name": "igorpf/php-di",
    "description": "PHP DI example tests",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Ferreira"
        }
    ],
    "config": {
      "optimize-autoloader": true
    },
    "require": {
        "php-di/php-di": "^6.0",
        "phake/phake": "^3.1"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0"
    },
    "autoload": {
      "psr-4": {
        "Example\\" : "src/"
      },
    "files": ["config.php"]
    },
    "autoload-dev": {
      "psr-0": {
        "ExampleTests\\": "tests/"
      }
    }
}
